#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

   long long l1,l2,r1,r2;
   cin>>l1>>r1>>l2>>r2;
   long long node1=min(r1,r2);
    long long node2=max(l1,l2);

    if( l2>r1 || r2 <l1)
        cout<<-1;
    else
    cout<<node2<<" "<<node1;

           return 0;
}
