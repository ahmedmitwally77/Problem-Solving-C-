#include <bits/stdc++.h>
using namespace std;
int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    map<char ,int> m;
    string s="codeforces";
    for (int i = 0; i < s.size(); ++i) {
        m[s[i]]++;
    }

    int t;cin>>t;
    while(t--){
        char a;cin>>a;
        cout<<((m[a])? "YES\n":"NO\n" );

    }



    return 0;
}
