#include <bits/stdc++.h>
using namespace std;
int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    long long arr[3];
    for (int i = 0; i < 3; ++i) {
        cin>>arr[i];
    }
    sort(arr,arr+3);
    if (arr[0] +arr[1] > arr[2])
        cout<<"0\n";
    else{
        cout<<arr[2]-(arr[0]+arr[1])+1<<"\n";
    }




    return 0;
}
