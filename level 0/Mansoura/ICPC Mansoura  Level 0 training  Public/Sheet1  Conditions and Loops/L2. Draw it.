#include <bits/stdc++.h>
using namespace std;
int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    char s;cin>>s;
    int n;cin>>n;
    while (n--){
        int num;cin>>num;
        for (int i = 0; i < num; ++i) {
            cout<<s;
        }
        cout<<"\n";
    }
    return 0;
}
