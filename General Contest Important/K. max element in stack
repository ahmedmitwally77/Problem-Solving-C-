//https://codeforces.com/group/3nQaj5GMG5/contest/373244/problem/K
#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    stack<long long> st, mx;
    long long t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        if (n == 1) {
            long long x;
            cin >> x;
            st.push(x);
            if (mx.empty()) {
                mx.push(x);
            } else {
                mx.push(max(x, mx.top()));
            }
        } else if (n == 2) {
            if (!st.empty()) {
                st.pop();
                mx.pop();
            }
        }

        if (!mx.empty()) {
            cout << mx.top() << "\n";
        }
    }

    return 0;
}
